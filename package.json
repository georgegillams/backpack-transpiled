{
  "name": "backpack-transpiled",
  "version": "0.0.6",
  "description": "Transpiled version of Skyscanner's design system (Backpack)",
  "main": "index.js",
  "scripts": {
    "pull-backpack": "npm run pull-backpack:clean && git clone git@github.com:Skyscanner/backpack && npm run pull-backpack:changelog && npm run pull-backpack:dependencies && npm run pull-backpack:build",
    "pull-backpack:clean": "rimraf backpack",
    "pull-backpack:changelog": "cp ./backpack/CHANGELOG.md ./BACKPACK_CHANGELOG.md",
    "pull-backpack:dependencies": "babel-node scripts/pull-dependencies.js",
    "pull-backpack:build": "(cd backpack && npm ci && npm run build && find packages -name \"node_modules\" -exec rm -rf {} \\; || true)",
    "prerelease": "npm run transpile",
    "release": "babel-node scripts/release.js",
    "release:major": "npm run release -- --major",
    "release:minor": "npm run release -- --minor",
    "release:patch": "npm run release -- --patch",
    "transpile": "npm run transpile:js && npm run transform:js && npm run transform:remove-bpk-no-touch && npm run transform:hierarchy && npm run transpile:remove-stories",
    "pretranspile": "npm run transpile:clean",
    "transpile:clean": "rimraf ./dist",
    "transpile:js": "babel backpack/packages -d dist --copy-files",
    "transform:js": "babel-node scripts/transform-js-scss-css-imports.js",
    "transform:remove-bpk-no-touch": "babel-node scripts/remove-bpk-no-touch.js",
    "transform:hierarchy": "babel-node scripts/transform-bpk-package-imports.js",
    "transpile:remove-stories": "find ./dist -name \"stories.js\" -exec rm {} \\;",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-proposal-private-methods": "^7.10.1",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-flow": "^7.10.4",
    "@babel/preset-react": "^7.10.1",
    "@babel/register": "^7.10.4",
    "add-asset-html-webpack-plugin": "3.1.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^7.0.2",
    "css-loader": "3.6.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-skyscanner-with-prettier": "^0.8.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "file-loader": "6.0.0",
    "node-sass": "^4.14.1",
    "node-sass-tilde-importer": "^1.0.2",
    "rimraf": "3.0.2",
    "sass": "^1.26.9",
    "sass-loader": "^9.0.0",
    "semver": "^7.3.2",
    "style-loader": "^1.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/georgegillams/backpack-transpiled.git"
  },
  "author": "georgegillams",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/georgegillams/backpack-transpiled/issues"
  },
  "homepage": "https://github.com/georgegillams/backpack-transpiled#readme",
  "dependencies": {
    "prop-types": "^15.6.2",
    "date-fns": "^1.29.0",
    "normalize.css": "4.2.0",
    "d3-path": "^1.0.5",
    "d3-scale": "^2.1.0",
    "lodash.debounce": "^4.0.8",
    "react-transition-group": "^2.5.3",
    "lodash.clamp": "^4.0.3",
    "color": "^3.0.0",
    "@skyscanner/popper.js": "^1.12.9-beta.1",
    "a11y-focus-scope": "^1.1.3",
    "a11y-focus-store": "^1.0.0",
    "lodash": "^4.17.12",
    "react-virtualized": "^9.20.1",
    "konami": "^1.6.2",
    "object-assign": "^4.1.1",
    "recompose": "^0.30.0",
    "tether": "^1.4.0",
    "react-google-maps": "^9.4.5",
    "react-autosuggest": "^10.0.2",
    "intersection-observer": "^0.7.0",
    "react-window": "^1.5.0",
    "react-responsive": "^5.0.0",
    "react-slider": "^1.0.1"
  }
}
