{
  "name": "backpack-transpiled",
  "version": "4.0.0",
  "description": "Transpiled version of Skyscanner's design system (Backpack)",
  "main": "./index.js",
  "scripts": {
    "pull-backpack": "npm run pull-backpack:clean && git clone https://github.com/Skyscanner/backpack.git && npm run pull-backpack:changelog && npm run pull-backpack:build",
    "pull-backpack:clean": "rimraf backpack",
    "pull-backpack:changelog": "cp ./backpack/CHANGELOG.md ./BACKPACK_CHANGELOG.md",
    "pull-backpack:build": "(cd backpack && PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=true npm ci && npm run build && (find packages -name \"node_modules\" -exec rm -rf {} \\; || true))",
    "prerelease": "npm run transpile",
    "release": "npm run release:checks && babel-node scripts/release.js",
    "release:major": "npm run release -- --major",
    "release:minor": "npm run release -- --minor",
    "release:patch": "npm run release -- --patch",
    "release:checks": "ls dist/bpk-component-icon/sm | grep accessibility",
    "pretranspile": "npm run transpile:clean",
    "transpile": "npm run transpile:js && npm run transpile-index && npm run transform:rename-css && npm run transform:js && npm run transform:remove-bpk-no-touch && npm run transform:hierarchy && npm run transpile:remove-stories && npm run transpile:generate-package-json && npm run transpile:copy-readme",
    "transpile:clean": "rimraf ./dist",
    "transpile:js": "babel backpack/packages -d dist --copy-files",
    "transpile-index": "cp index.js dist/index.js",
    "transform:rename-css": "babel-node scripts/rename-css-modules.js",
    "transform:js": "babel-node scripts/transform-js-scss-css-imports.js",
    "transform:remove-bpk-no-touch": "babel-node scripts/remove-bpk-no-touch.js",
    "transform:hierarchy": "babel-node scripts/transform-bpk-package-imports.js",
    "transpile:remove-stories": "find ./dist -name \"stories.js\" -exec rm {} \\;",
    "transpile:generate-package-json": "babel-node scripts/generate-package-json.js",
    "transpile:copy-readme": "cp README.md ./dist/",
    "lint": "npm run lint:js",
    "lint:js": "eslint scripts --fix && eslint test --fix",
    "test": "cross-env jest --coverage"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-proposal-private-methods": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-flow": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.11.5",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^12.0.0",
    "add-asset-html-webpack-plugin": "3.1.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^7.0.2",
    "css-loader": "4.3.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-skyscanner-with-prettier": "^0.8.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "file-loader": "6.0.0",
    "jest": "^26.2.2",
    "node-sass": "^4.14.1",
    "node-sass-tilde-importer": "^1.0.2",
    "react": "^16.14.0",
    "react-dom": "^16.13.1",
    "rimraf": "3.0.2",
    "sass": "^1.26.9",
    "sass-loader": "^10.0.2",
    "semver": "^7.3.2",
    "style-loader": "^1.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/georgegillams/backpack-transpiled.git"
  },
  "author": "georgegillams",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/georgegillams/backpack-transpiled/issues"
  },
  "homepage": "https://github.com/georgegillams/backpack-transpiled#readme"
}
